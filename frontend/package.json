{
  "name": "frontend",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_DEBUG=true next dev",
    "dev:mocked": "NEXT_PUBLIC_MOCK_API=true npm run dev",
    "watch": "chokidar \"src/**/*\" \"next.config.js\" --initial --command=\"npm run watch_build\"",
    "watch_build": "NEXT_PUBLIC_DEBUG=true npm run build",
    "build": "next build && next export",
    "build:mocked": "NEXT_PUBLIC_MOCK_API=true NEXT_PUBLIC_DEBUG=true npm run build",
    "start": "next start",
    "lint": "stylelint '**/*.scss' && prettier --check './src' && next lint",
    "licensecheck": "license-checker --excludePrivatePackages --production --onlyAllow 'MPL; MIT; BSD-3-Clause; BSD-2-Clause; 0BSD; Apache-2.0; ISC; Zlib; CC0; Unlicense; WTFPL;'",
    "test": "jest",
    "postinstall": "msw init public/ --save",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@fluent/langneg": "^0.6.1",
    "@fluent/react": "^0.14.1",
    "@mozilla-protocol/core": "^16.0.0",
    "intl-pluralrules": "^1.3.1",
    "react": "17.0.2",
    "react-aria": "^3.14.0",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-intersection-observer": "^8.33.1",
    "react-stately": "^3.12.2",
    "react-toastify": "^8.2.0",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.1.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest-axe": "^3.5.3",
    "@types/react": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-jest": "^27.5.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "jest-junit": "^13.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.3.6",
    "msw": "^0.39.0",
    "next": "^12.1.0",
    "prettier": "2.6.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.6.2"
  },
  "volta": {
    "node": "14.18.1",
    "npm": "8.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}