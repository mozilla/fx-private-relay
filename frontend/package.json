{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "watch": "chokidar \"src/**/*\" \"next.config.js\" --initial --command=\"npm run watch_build\"",
    "watch_build": "NODE_ENV='watch_build' npm run build",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.0",
    "@fluent/langneg": "^0.6.0",
    "@fluent/react": "^0.14.0",
    "@mozilla-protocol/core": "^16.0.0",
    "intl-pluralrules": "^1.3.1",
    "next": "^12.0.7",
    "react": "17.0.2",
    "react-aria": "^3.10.0",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-intersection-observer": "^8.32.5",
    "react-stately": "^3.9.0",
    "react-toastify": "^8.1.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest-axe": "^3.5.3",
    "@types/react": "17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "chokidar-cli": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-testing-library": "^5.0.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.7",
    "typescript": "^4.4.4"
  },
  "volta": {
    "node": "14.18.1",
    "npm": "8.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write"
  }
}
