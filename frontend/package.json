{
  "name": "frontend",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_DEBUG=true next dev",
    "dev:mocked": "NEXT_PUBLIC_MOCK_API=true npm run dev",
    "watch": "chokidar \"src/**/*\" \"next.config.js\" --initial --command=\"npm run watch_build\"",
    "watch_build": "NEXT_PUBLIC_DEBUG=true npm run build",
    "build": "next build && next export",
    "build:mocked": "NEXT_PUBLIC_MOCK_API=true NEXT_PUBLIC_DEBUG=true npm run build",
    "start": "next start",
    "lint": "stylelint '**/*.scss' && prettier --check './src' && next lint",
    "licensecheck": "license-checker --excludePrivatePackages --production --onlyAllow 'MPL; MIT; BSD-3-Clause; BSD-2-Clause; 0BSD; Apache-2.0; ISC; Zlib; CC0; Unlicense; WTFPL; Unicode-DFS-2016;'",
    "test": "jest",
    "postinstall": "msw init public/ --save",
    "prepare": "if [ \"$ON_HEROKU\" != \"True\" ]; then cd .. && husky install frontend/.husky; fi"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@fluent/langneg": "^0.6.2",
    "@fluent/react": "^0.14.1",
    "@mozilla-protocol/core": "^16.0.1",
    "cldr-localenames-modern": "^42.0.0",
    "intl-pluralrules": "^1.3.1",
    "msw": "^0.49.2",
    "react": "18.2.0",
    "react-aria": "^3.22.0",
    "react-dom": "18.2.0",
    "react-ga": "^3.3.1",
    "react-intersection-observer": "^9.4.1",
    "react-stately": "^3.20.0",
    "react-toastify": "^9.1.1",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.1.1",
    "@next/eslint-plugin-next": "^13.0.5",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest-axe": "^3.5.5",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-jest": "^29.3.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-testing-library": "^5.9.1",
    "fast-check": "^3.5.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^13.1.0",
    "next": "^13.1.1",
    "prettier": "2.8.2",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4"
  },
  "volta": {
    "node": "14.20.1",
    "npm": "8.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
